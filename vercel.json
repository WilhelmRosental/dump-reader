{
  "version": 2,
  "builds": [
    {
      "src": "dist/dump-reader/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/dump-reader/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "dist/dump-reader/browser/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "dist/dump-reader/browser/favicon.ico"
    },
    {
      "src": "/(.*\\.(css|js))",
      "dest": "dist/dump-reader/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/dump-reader/server/server.mjs"
    }
  ]
}
